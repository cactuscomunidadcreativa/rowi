{
  "name": "rowi-sia-app",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "NEXT_DISABLE_TURBOPACK=1 next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "sync:i18n": "pnpm dlx tsx scripts/sync-i18n-with-db.ts",
    "sync:i18n:import": "pnpm dlx tsx scripts/sync-i18n-with-db.ts --import",
    "sync:i18n:export": "pnpm dlx tsx scripts/sync-i18n-with-db.ts",
    "sync:i18n:merge": "pnpm dlx tsx scripts/sync-i18n-with-db.ts --merge",
    "import:csv": "node scripts/import-csv.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "tsx prisma/seed.ts",
    "dev:prep": "npm run db:generate && npm run db:migrate -- --name init && npm run db:seed",
    "dev:all": "npm run dev:prep && NEXT_DISABLE_TURBOPACK=1 next dev -p 3000",
    "cron:affinity": "ts-node src/lib/cron/affinity.cron.ts",
    "map:files": "npx tree -L 3 -I 'node_modules|.next|.git|coverage|dist'",
    "map:routes": "node scripts/map-routes.mjs",
    "map:ai": "node scripts/scan-ai.mjs",
    "map:deps": "node scripts/map-deps.mjs",
    "detect:translations": "tsx scripts/detect.translations.deep.ts",
    "reset:rowi": "rm -rf .next node_modules && pnpm store prune && pnpm install && pnpm prisma generate && pnpm build && pnpm dev"
  },
  "dependencies": {
    "...": "link:...",
    "@auth/prisma-adapter": "2.11.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "6.17.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sanity/client": "7.11.0",
    "@sanity/image-url": "1.2.0",
    "@sanity/vision": "4.6.1",
    "@swc/helpers": "0.5.17",
    "@tanstack/react-table": "8.21.3",
    "bcrypt": "6.0.0",
    "bcryptjs": "3.0.2",
    "chalk": "^5.6.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "csv-parse": "5.6.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "6.6.0",
    "d3-geo": "^3.1.1",
    "framer-motion": "12.23.22",
    "lucide-react": "0.544.0",
    "next": "15.5.2",
    "next-auth": "4.24.11",
    "next-sanity": "10.0.15",
    "openai": "4.104.0",
    "papaparse": "5.5.3",
    "pg": "8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-simple-maps": "^3.0.0",
    "recharts": "3.2.1",
    "resend": "^6.4.0",
    "sanity": "^4.13.0",
    "sonner": "2.0.7",
    "sortablejs": "^1.15.6",
    "string_decoder": "1.3.0",
    "swr": "2.3.6",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "3.1.1",
    "tailwindcss-animate": "1.0.7",
    "topojson-client": "^3.1.0",
    "tslib": "2.8.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.12",
    "@types/node": "24.5.0",
    "@types/papaparse": "5.3.16",
    "@types/react": "19.2.2",
    "@types/sortablejs": "^1.15.9",
    "autoprefixer": "10.4.21",
    "dependency-cruiser": "17.0.2",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "9.34.0",
    "eslint-config-next": "15.5.2",
    "madge": "8.0.0",
    "postcss": "8.5.6",
    "prisma": "6.17.1",
    "tailwindcss": "3.4.17",
    "tree-cli": "0.6.7",
    "ts-node": "10.9.2",
    "tsx": "4.20.6",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "neverBuiltDependencies": [],
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    },
    "packageExtensions": {}
  }
}